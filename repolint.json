              "Solutions\/NLPSolution1-QuestionAnswering\/QuestionAnswering\/bert\/src\/main\/java\/com\/qualcomm\/qti\/qa",
              "Solutions\/NLPSolution2-SentimentAnalysis\/SentimentAnalysis\/app\/src\/main\/java\/com\/qualcomm\/qti\/sentimentanalysis\/tokenization\/",
              "Solutions\/VisionSolution1-ObjectDetection-DETR\/app\/src\/main\/cpp\/inc\/hpp\/",
              "Solutions\/VisionSolution1-ObjectDetection-YoloNas\/app\/src\/main\/cpp\/inc\/hpp\/",
              "Solutions\/VisionSolution4-PoseEstimation\/app\/src\/main\/cpp\/inc\/hpp\/",
              "Examples\/Python-ASR-wav2vec2\/automatic_speech_recognition_android_app\/app\/src\/main\/cpp\/inc\/hpp\/",
              "Solutions\/NLPSolution1-QuestionAnswering\/QuestionAnswering\/bert\/src\/main\/cpp\/inc\/hpp\/",
              "Solutions\/NLPSolution2-SentimentAnalysis\/SentimentAnalysis\/app\/src\/main\/cpp\/inc\/hpp\/",
              "Solutions\/NLPSolution3-AutomaticSpeechRecognition-Whisper\/Android_App_Whisper\/app\/src\/main\/cpp\/inc\/hpp\/",
