            "patterns": [
              "babel.config.js",
              "build\/",
              "jest.config.js",
              "node_modules\/",
              "types\/",
              "uthash.h",
              "Solutions\/NLPSolution3-AutomaticSpeechRecognition-Whisper\/Android_App_Whisper\/app\/src\/main\/cpp\/",
              "Solutions\/VisionSolution1-ObjectDetection\/app\/src\/main\/java\/com\/qc\/objectdetection\/",
